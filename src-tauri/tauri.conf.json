	"productName": "vxfetch",
	"version": "1.0.0",
	"identifier": "me.yanice.vxfetch",
	"build": {
		"beforeDevCommand": "pnpm dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "pnpm build",
		"frontendDist": "../dist"
	},
